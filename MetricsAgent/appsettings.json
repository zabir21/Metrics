{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Settings": {
    "Logger": {
      "Path": "C:/logs/MetricsAgent"
    },
    "DatabaseOptions": {
      "ConnectionString": "Data Source = metrics.db; Version = 3;  Pooling= true; MaxPool Size = 100;"
    }
  }
}
